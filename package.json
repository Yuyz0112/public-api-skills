{
	"name": "public-api-skills",
	"private": true,
	"description": "Public APIs as Agent Skills format",
	"scripts": {
		"generate:asana": "bunx openapi-to-skills ./apis/asana/openapi.yaml -o ./apis/asana -f",
		"generate:cloudflare": "bunx openapi-to-skills ./apis/cloudflare/openapi.json -o ./apis/cloudflare -f",
		"generate:discord": "bunx openapi-to-skills ./apis/discord/openapi.json -o ./apis/discord -f",
		"generate:figma": "bunx openapi-to-skills ./apis/figma/openapi.yaml -o ./apis/figma -f",
		"generate:gitlab": "bunx openapi-to-skills ./apis/gitlab/openapi.yaml -o ./apis/gitlab -f",
		"generate:github": "bunx openapi-to-skills ./apis/github/openapi.json -o ./apis/github -f",
		"generate:jira-cloud": "bunx openapi-to-skills ./apis/jira-cloud/openapi.json -o ./apis/jira-cloud -f",
		"generate:jira-server": "bunx openapi-to-skills ./apis/jira-server/openapi.json -o ./apis/jira-server -f",
		"generate:pagerduty": "bunx openapi-to-skills ./apis/pagerduty/openapi.json -o ./apis/pagerduty -f",
		"generate:shopify": "bunx openapi-to-skills ./apis/shopify/openapi.json -o ./apis/shopify -f",
		"generate:slack": "bunx openapi-to-skills ./apis/slack/openapi.json -o ./apis/slack -f",
		"generate:stripe": "bunx openapi-to-skills ./apis/stripe/openapi.json -o ./apis/stripe -f",
		"generate:testrail": "bunx openapi-to-skills ./apis/testrail/openapi.yaml -o ./apis/testrail --includeTags projects,suites,sections,cases,runs,plans,tests,results,statuses,users,milestones,attachments -f",
		"generate:testrail:readonly": "bunx openapi-to-skills ./apis/testrail/openapi.yaml -o ./apis/testrail --name testrail-api-readonly --includeTags read -f",
		"generate:twilio": "bunx openapi-to-skills ./apis/twilio/openapi.json -o ./apis/twilio -f",
		"generate:twitter": "bunx openapi-to-skills ./apis/twitter/openapi.json -o ./apis/twitter -f"
	},
	"devDependencies": {
		"openapi-to-skills": "^0.2.0"
	}
}
